function init_test_cubes()
  local red, yellow, green, blue = 0, 1, 2, 3
  scene:add_cube(4, 4, 0, red)
  scene:add_cube(4, -4, 0, yellow)
  scene:add_cube(-4, -4, 0, green)
  scene:add_cube(-4, 4, 0, blue)

  scene:add_cube(2, 1, 0, yellow)
  scene:add_cube(2, 3, 0, yellow)
  scene:add_cube(1, 2, 0, yellow)
  scene:add_cube(3, 2, 0, yellow)

  scene:add_cube(2, -1, 0, green)
  scene:add_cube(2, -3, 0, green)
  scene:add_cube(1, -2, 0, green)
  scene:add_cube(3, -2, 0, green)

  scene:add_cube(-2, -1, 0, blue)
  scene:add_cube(-2, -3, 0, blue)
  scene:add_cube(-1, -2, 0, blue)
  scene:add_cube(-3, -2, 0, blue)

  scene:add_cube(-2, 1, 0, red)
  scene:add_cube(-2, 3, 0, red)
  scene:add_cube(-1, 2, 0, red)
  scene:add_cube(-3, 2, 0, red)

  scene:add_cube(2, 2, 0, red)
  scene:add_cube(2, -2, 0, yellow)
  scene:add_cube(-2, -2, 0, green)
  scene:add_cube(-2, 2, 0, blue)
end

function init_rnd_cubes()
  for i = -4, 4, 1 do
    for j = -4, 4, 1 do
      if 1 > rnd(8) and (i != 4 or j != 4) then
        col = flr(rnd(4))
        --(i + j)%6
        sss = false
        --rnd(5) < 1
        for z = 0, flr(rnd(5)) do
          scene:add_cube(i, j, z, flr(rnd(4)))
        end
      end
    end
  end

  scene:sort_cubes()
end

function _init()
  -- btn(l,r,u,d)
  dirx = { -1, 1, 0, 0 }
  diry = { 0, 0, 1, -1 }

  -- init_test_cubes()
  init_rnd_cubes()

  _upd_player = update_player
end

function mov_walk()
  potter.ox = potter.sox * (1 - potter.pt)
  potter.oy = potter.soy * (1 - potter.pt)
end

function mov_bump()
  local pt = potter.pt
  if pt >= 0.5 then
    pt = 1 - pt
  end
  potter.ox = potter.sox * pt
  potter.oy = potter.soy * pt
end

function update_pturn()
  potter.pt = min(potter.pt + 0.2, 1)

  potter.mov()

  if potter.pt == 1 then
    _upd_player = update_player
  end
end

function move_player(dx, dy)
  local px, py = potter.x + dx, potter.y + dy
  local loc = loc_hash(px, py, 0)
  local cube = scene.cubes[scene.locations[loc]]
  potter.dir = dx * 2 + dy

  if not scene:try_push(px, py, dx, dy) then
    -- bump
    potter.sox, potter.soy = dx * 3, dy * 3
    potter.ox, potter.oy = 0, 0
    potter.pt = 0
    potter.mov = mov_bump
  else
    -- walk
    potter.x += dx
    potter.y += dy
    potter.sox, potter.soy = -dx * 4, -dy * 4
    potter.ox, potter.oy = potter.sox, potter.soy
    potter.pt = 0

    potter.mov = mov_walk
  end
  _upd_player = update_pturn
end

function update_player()
  for i = 0, 3 do
    if btnp(i) then
      local dx, dy = camera:normalized_xy(dirx[i + 1], diry[i + 1])
      -- flip controls when viewing from some directions (manual test)
      if (camera.dir + 2) % 8 > 3 then
        dx = -dx
        dy = -dy
      end
      move_player(dx, dy)
      return
    end
  end
end

function _update60()
  -- rotate indefinitely
  -- if t%8 == 0 then rotate_z(1) end
  _upd_player()
  if btnp(4) then
    camera:rotate_z(1)
  end
  if btnp(5) then
    camera:rotate_z(-1)
  end
end

