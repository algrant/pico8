  { [5] = 14, [6] = 8, [7] = 2 }, -- red
  { [5] = 4, [6] = 10, [7] = 9 }, -- yellow
  { [5] = 5, [6] = 11, [7] = 3 }, -- green
  { [5] = 5, [6] = 12, [7] = 15 } -- blue
}


function draw_potter()
  local pp = camera:screen_xy(potter.x + potter.ox / 4, potter.y + potter.oy / 4, potter.z)
  local anims = {
    { 136, 138 },
    { 168, 170 }
  }

  local opts = {
    [2] = { 136, false },
    [-1] = { 136, true },
    [-2] = { 168, true },
    [1] = { 168, false }
  }
  -- need more thought on directions sadly...

  spr(128, pp.x, pp.y, 2, 2, opts[potter.dir][2])
end

function draw_board()
  local rp = camera:rp()

  for i = -4, 4 do
    for j = -4, 4 do
      local c2d = camera:screen_xy(i, j, -1)
      local p = tonum(not (i % 3 == 0 or j % 3 == 0))
      local sid = rp[10] + p * 16
      spr(sid, c2d.x, c2d.y, 2, 1)
    end
  end
end

function _draw()
  t += 1
  f = flr(t / 2.5)
  rectfill(0, 0, 127, 127, 1)
  rectfill(1, 1, 126, 126, 0)
  draw_board()
  print(camera.dir, 0, 0, 8)
  local pd = false
  local potter_distance = camera:camera_dist(potter.x + potter.ox / 4, potter.y + potter.oy / 4, 0)
  for cube_id in all(scene.ordered) do
    local cube = scene.cubes[cube_id]
    if not pd and potter_distance - cube:screen_distance() < 0 then
      draw_potter()
      pd = true
    end
    draw_scene_cube(cube)
  end
  if not pd then draw_potter() end
end

